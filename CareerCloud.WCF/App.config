<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>

    <services>

      
      
      
      
      <service name="CareerCloud.WCF.Applicant">
        <clear />

        
        <endpoint 
              binding="basicHttpBinding" 
              name=""
              contract="CareerCloud.WCF.IApplicant" 
              listenUriMode="Explicit"
          >
          <identity>
            <dns value="localhost" />
            </identity>
        </endpoint>

        <endpoint 
              address="mex" 
              binding="mexHttpBinding" 
              name=""
              contract="IMetadataExchange" 
              listenUriMode="Explicit"
          >
          <identity>
            
          </identity>
        </endpoint>


        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/CareerCloud.WCF/Applicant/" />
          </baseAddresses>
        </host>
      </service>





      <service name="CareerCloud.WCF.Company">
        <clear />


        <endpoint
          binding="basicHttpBinding"
          name=""
          contract="CareerCloud.WCF.ICompany"
          listenUriMode="Explicit"
        >
          <identity>
            <dns value="localhost" />
            
          </identity>
        </endpoint>

        <endpoint
          address="mex"
          binding="mexHttpBinding"
          name=""
          contract="IMetadataExchange"
          listenUriMode="Explicit"
        >
          <identity>
            
          </identity>
        </endpoint>


        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/CareerCloud.WCF/Company/" />
          </baseAddresses>
        </host>
      </service>




      <service name="CareerCloud.WCF.Security">
        <clear />


        <endpoint
          binding="basicHttpBinding"
          name=""
          contract="CareerCloud.WCF.ISecurity"
          listenUriMode="Explicit"
        >
          <identity>
            <dns value="localhost" />
           
          </identity>
        </endpoint>

        <endpoint
          address="mex"
          binding="mexHttpBinding"
          name=""
          contract="IMetadataExchange"
          listenUriMode="Explicit"
        >
          <identity>
           
          </identity>
        </endpoint>


        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/CareerCloud.WCF/Security/" />
          </baseAddresses>
        </host>
      </service>










      <service name="CareerCloud.WCF.System">
        <clear />


        <endpoint
          binding="basicHttpBinding"
          name=""
          contract="CareerCloud.WCF.ISystem"
          listenUriMode="Explicit"
        >
          <identity>
            <dns value="localhost" />
           
          </identity>
        </endpoint>

        <endpoint
          address="mex"
          binding="mexHttpBinding"
          name=""
          contract="IMetadataExchange"
          listenUriMode="Explicit"
        >
          <identity>
            
          </identity>
        </endpoint>


        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/CareerCloud.WCF/System/" />
          </baseAddresses>
        </host>
      </service>


















    </services>


    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, 
          set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True"/>
          <!-- To receive exception details in faults for debugging purposes, 
          set the value below to true.  Set to false before deployment 
          to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

</configuration>
